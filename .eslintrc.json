{
  "root": true,
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": [["@babel/plugin-syntax-import-attributes", { "deprecatedAssertSyntax": true }]]
    }
  },
  "env": {
    "es2021": true,
    "jest": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-console": "error"
  }
}
